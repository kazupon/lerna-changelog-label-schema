{
  "name": "lerna-changelog-label-schema",
  "description": "Label schema definition for lerna-changelog",
  "version": "2.0.2",
  "author": {
    "name": "kazuya kawaguchi",
    "email": "kawakazu80@gmail.com"
  },
  "bin": "bin/run.js",
  "changelog": {
    "labels": {
      "Type: Feature": ":star: Features",
      "Type: Bug": ":bug: Bug Fix",
      "Type: Security": ":lock: Security Fix",
      "Type: Performance": ":chart_with_upwards_trend: Performance Fix",
      "Type: Improvement": ":zap: Improvement Feature",
      "Type: Breaking": ":boom: Breaking Change",
      "Type: Deprecated": ":warning: Deprecated Feature",
      "Type: I18n": ":globe_with_meridians: Internationalization",
      "Type: A11y": ":wheelchair: Accessibility",
      "Type: Refactoring": ":shirt: Refactoring",
      "Type: Testing": ":white_check_mark: Testing",
      "Type: Documentation": ":pencil: Documentation"
    }
  },
  "dependencies": {
    "meow": "^5.0.0"
  },
  "devDependencies": {
    "@types/jest": "^24.0.23",
    "@types/meow": "^5.0.0",
    "@types/node": "^12.12.14",
    "@typescript-eslint/eslint-plugin": "^2.9.0",
    "@typescript-eslint/parser": "^2.9.0",
    "@typescript-eslint/typescript-estree": "^2.9.0",
    "eslint": "^6.7.1",
    "eslint-plugin-vue-libs": "^4.0.0",
    "jest": "^24.9.0",
    "lerna-changelog": "^0.8.3",
    "opener": "^1.5.1",
    "shipjs": "^0.11.0",
    "ts-jest": "^24.2.0",
    "typescript": "^3.7.2"
  },
  "engines": {
    "node": ">= 8"
  },
  "files": [
    "bin",
    "lib",
    "preset"
  ],
  "homepage": "https://github.com/kazupon/lerna-changelog-label-schema#readme",
  "keywords": [
    "labels",
    "lerna-changelog"
  ],
  "license": "MIT",
  "main": "lib/cli.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kazupon/lerna-changelog-label-schema.git"
  },
  "scripts": {
    "build": "tsc -p .",
    "clean": "rm -rf ./coverage && rm -rf ./lib/*.js*",
    "coverage": "opener coverage/lcov-report/index.html",
    "lint": "eslint ./src --ext .ts",
    "release:prepare": "shipjs prepare",
    "release:trigger": "shipjs trigger",
    "test": "npm run lint && npm run test:cover",
    "test:cover": "npm run test:unit -- --coverage",
    "test:unit": "jest --env node",
    "watch": "tsc -p . --watch"
  },
  "types": "types/index.d.ts"
}
